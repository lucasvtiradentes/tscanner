{
  "name": "tscanner-vscode",
  "private": true,
  "displayName": "TScanner",
  "description": "Code quality scanner for the AI-generated code era",
  "version": "0.0.34",
  "publisher": "lucasvtiradentes",
  "license": "MIT",
  "icon": "resources/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvtiradentes/tscanner"
  },
  "bugs": {
    "url": "https://github.com/lucasvtiradentes/tscanner/issues"
  },
  "homepage": "https://github.com/lucasvtiradentes/tscanner#readme",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Linters",
    "Programming Languages",
    "Formatters",
    "Testing"
  ],
  "keywords": [
    "typescript",
    "linter",
    "code-quality",
    "scanner",
    "static-analysis",
    "eslint",
    "biome",
    "rust",
    "tsx",
    "javascript",
    "custom-rules",
    "ai-validation",
    "git-integration",
    "branch-scanning",
    "code-review",
    "diagnostics"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "main": "./out/extension.js",
  "scripts": {
    "build": "tsx esbuild.config.ts",
    "lint": "biome check src",
    "lint:fix": "biome check --fix --unsafe src",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build",
    "vscode:package": "vsce package --no-dependencies",
    "vscode:publish": "vsce publish --no-dependencies"
  },
  "dependencies": {
    "jsonc-parser": "^3.3.1",
    "vscode-languageclient": "^9.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.93.0",
    "esbuild": "^0.24.0",
    "tscanner-common": "workspace:*",
    "tsx": "^4.20.6",
    "typescript": "^5.7.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tscanner",
          "title": "tscanner",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "tscanner": [
        {
          "id": "tscannerExplorer",
          "name": "Issues",
          "icon": "resources/icon.svg"
        },
        {
          "id": "tscannerAiExplorer",
          "name": "AI Issues",
          "icon": "$(hubot)"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "tscannerExplorer",
        "contents": "Searching for issues...",
        "when": "workbenchState != empty && tscannerSearching"
      },
      {
        "view": "tscannerExplorer",
        "contents": "Click the refresh button to run scan.",
        "when": "workbenchState != empty && !tscannerSearching && !tscannerHasScanned"
      },
      {
        "view": "tscannerExplorer",
        "contents": "No issues found.",
        "when": "workbenchState != empty && !tscannerSearching && tscannerHasScanned"
      },
      {
        "view": "tscannerExplorer",
        "contents": "Open a folder to start scanning for issues.\n[Open Folder](command:vscode.openFolder)",
        "when": "workbenchState == empty"
      },
      {
        "view": "tscannerAiExplorer",
        "contents": "Searching for AI issues...",
        "when": "workbenchState != empty && tscannerAiSearching"
      },
      {
        "view": "tscannerAiExplorer",
        "contents": "Click the refresh button to run AI scan.",
        "when": "workbenchState != empty && !tscannerAiSearching && !tscannerHasAiScanned"
      },
      {
        "view": "tscannerAiExplorer",
        "contents": "No AI issues found.",
        "when": "workbenchState != empty && !tscannerAiSearching && tscannerHasAiScanned"
      },
      {
        "view": "tscannerAiExplorer",
        "contents": "Open a folder to start scanning for AI issues.\n[Open Folder](command:vscode.openFolder)",
        "when": "workbenchState == empty"
      }
    ],
    "commands": [
      {
        "command": "tscanner.refreshIssues",
        "title": "tscanner: Refresh Issues",
        "shortTitle": "Refresh Issues",
        "icon": "$(refresh)"
      },
      {
        "command": "tscanner.goToNextIssue",
        "title": "tscanner: Go to Next Issue",
        "icon": "$(arrow-down)"
      },
      {
        "command": "tscanner.goToPreviousIssue",
        "title": "tscanner: Go to Previous Issue",
        "icon": "$(arrow-up)"
      },
      {
        "command": "tscanner.showLogs",
        "title": "tscanner: Show Logs",
        "icon": "$(output)"
      },
      {
        "command": "tscanner.openSettingsMenu",
        "title": "Open Settings"
      },
      {
        "command": "tscanner.cycleViewModeFileFlatView",
        "title": "Group by file, flat view",
        "icon": "$(list-flat)"
      },
      {
        "command": "tscanner.cycleViewModeFileTreeView",
        "title": "Group by file, tree view",
        "icon": "$(list-tree)"
      },
      {
        "command": "tscanner.cycleViewModeRuleFlatView",
        "title": "Group by rule, flat view",
        "icon": "$(group-by-ref-type)"
      },
      {
        "command": "tscanner.cycleViewModeRuleTreeView",
        "title": "Group by rule, tree view",
        "icon": "$(type-hierarchy)"
      },
      {
        "command": "tscanner.openFile",
        "title": "Open File"
      },
      {
        "command": "tscanner.copyRuleIssues",
        "title": "Copy Rule Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.copyFileIssues",
        "title": "Copy File Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.copyFolderIssues",
        "title": "Copy Folder Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.copyAllIssues",
        "title": "Copy All Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.refreshAiIssues",
        "title": "tscanner: Refresh AI Issues",
        "shortTitle": "Refresh AI Issues",
        "icon": "$(refresh)"
      },
      {
        "command": "tscanner.copyAiRuleIssues",
        "title": "Copy AI Rule Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.copyAiFileIssues",
        "title": "Copy AI File Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.copyAiFolderIssues",
        "title": "Copy AI Folder Issues",
        "icon": "$(copy)"
      },
      {
        "command": "tscanner.copyAllAiIssues",
        "title": "Copy All AI Issues",
        "icon": "$(copy)"
      }
    ],
    "keybindings": [],
    "menus": {
      "view/title": [
        {
          "command": "tscanner.copyAllIssues",
          "when": "view == tscannerExplorer",
          "group": "navigation@5"
        },
        {
          "command": "tscanner.refreshIssues",
          "when": "view == tscannerExplorer && !tscannerSearching",
          "group": "navigation@1"
        },
        {
          "command": "tscanner.cycleViewModeFileFlatView",
          "when": "view == tscannerExplorer && tscannerViewMode == list && tscannerGroupMode == file",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.cycleViewModeFileTreeView",
          "when": "view == tscannerExplorer && tscannerViewMode == tree && tscannerGroupMode == file",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.cycleViewModeRuleFlatView",
          "when": "view == tscannerExplorer && tscannerViewMode == list && tscannerGroupMode == rule",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.cycleViewModeRuleTreeView",
          "when": "view == tscannerExplorer && tscannerViewMode == tree && tscannerGroupMode == rule",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.copyAllAiIssues",
          "when": "view == tscannerAiExplorer",
          "group": "navigation@5"
        },
        {
          "command": "tscanner.refreshAiIssues",
          "when": "view == tscannerAiExplorer && !tscannerAiSearching",
          "group": "navigation@1"
        },
        {
          "command": "tscanner.cycleViewModeFileFlatView",
          "when": "view == tscannerAiExplorer && tscannerViewMode == list && tscannerGroupMode == file",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.cycleViewModeFileTreeView",
          "when": "view == tscannerAiExplorer && tscannerViewMode == tree && tscannerGroupMode == file",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.cycleViewModeRuleFlatView",
          "when": "view == tscannerAiExplorer && tscannerViewMode == list && tscannerGroupMode == rule",
          "group": "navigation@20"
        },
        {
          "command": "tscanner.cycleViewModeRuleTreeView",
          "when": "view == tscannerAiExplorer && tscannerViewMode == tree && tscannerGroupMode == rule",
          "group": "navigation@20"
        }
      ],
      "view/item/context": [
        {
          "command": "tscanner.copyRuleIssues",
          "when": "view == tscannerExplorer && viewItem == TscannerNodeRuleGroup",
          "group": "inline"
        },
        {
          "command": "tscanner.copyFileIssues",
          "when": "view == tscannerExplorer && viewItem == TscannerNodeFile && tscannerGroupMode == file",
          "group": "inline"
        },
        {
          "command": "tscanner.copyFolderIssues",
          "when": "view == tscannerExplorer && viewItem == TscannerNodeFolder && tscannerGroupMode == file",
          "group": "inline"
        },
        {
          "command": "tscanner.copyAiRuleIssues",
          "when": "view == tscannerAiExplorer && viewItem == TscannerNodeRuleGroup",
          "group": "inline"
        },
        {
          "command": "tscanner.copyAiFileIssues",
          "when": "view == tscannerAiExplorer && viewItem == TscannerNodeFile && tscannerGroupMode == file",
          "group": "inline"
        },
        {
          "command": "tscanner.copyAiFolderIssues",
          "when": "view == tscannerAiExplorer && viewItem == TscannerNodeFolder && tscannerGroupMode == file",
          "group": "inline"
        }
      ],
      "commandPalette": [
        {
          "command": "tscanner.openSettingsMenu",
          "when": "false"
        },
        {
          "command": "tscanner.cycleViewModeFileFlatView",
          "when": "false"
        },
        {
          "command": "tscanner.cycleViewModeFileTreeView",
          "when": "false"
        },
        {
          "command": "tscanner.cycleViewModeRuleFlatView",
          "when": "false"
        },
        {
          "command": "tscanner.cycleViewModeRuleTreeView",
          "when": "false"
        },
        {
          "command": "tscanner.openFile",
          "when": "false"
        },
        {
          "command": "tscanner.copyRuleIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyFileIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyFolderIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyAllIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyAiRuleIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyAiFileIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyAiFolderIssues",
          "when": "false"
        },
        {
          "command": "tscanner.copyAllAiIssues",
          "when": "false"
        }
      ]
    },
    "configuration": {
      "title": "TScanner",
      "properties": {
        "tscanner.lsp.bin": {
          "type": "string",
          "default": "",
          "markdownDescription": "Path to the `tscanner` binary. Leave empty to auto-detect from `node_modules` or global installation.\n\nExamples:\n- `/usr/local/bin/tscanner`\n- `~/bin/tscanner`\n- `./node_modules/.bin/tscanner` (relative to workspace)",
          "scope": "machine-overridable"
        },
        "tscanner.logs.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable logging to an output channel for debugging purposes (restart required)."
        }
      }
    }
  }
}
